{
  "name": "autotourism",
  "version": "8.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssr": "ng run autotourism:serve-ssr",
    "ssr:watch": "ng run autotourism:serve-ssr:dev",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "build": "ng build",
    "build:server": "ng run autotourism:server:production",
    "build:prod": "ng build --prod",
    "build:universal": "ng run autotourism:build:production && ng run autotourism:server:production && webpack --progress --colors",
    "server": "node dist-server/main.js",
    "test": "ng test",
    "test:ssr": "run-p test:ssr:*",
    "test:ssr:server": "cross-env TEST=true node server.js",
    "test:ssr:script": "node ./node_modules/npm-delay 2000 && ts-mocha test/*.spec.ts test/exit.ts --timeout 15000",
    "lint": "ng lint",
    "dev:ssr": "ng run autotourism:serve-ssr",
    "serve:ssr": "node dist-server/main.js",
    "build:ssr": "ng build --prod && ng run autotourism:server:production",
    "prepare-routes": "tsc prerender && node prerender",
    "prerender": "ng run autotourism:prerender",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.14",
    "@angular/common": "11.2.14",
    "@angular/compiler": "11.2.14",
    "@angular/core": "11.2.14",
    "@angular/forms": "11.2.14",
    "@angular/platform-browser": "11.2.14",
    "@angular/platform-browser-dynamic": "11.2.14",
    "@angular/pwa": "0.1102.18",
    "@angular/router": "11.2.14",
    "@angular/service-worker": "11.2.14",
    "@gorniv/ngx-universal": "^2.2.2",
    "@nguniversal/common": "^11.2.1",
    "@nguniversal/express-engine": "^11.2.1",
    "@ngx-meta/core": "9.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@smip/ngx-materialize": "^0.3.0",
    "angulartics2": "^10.1.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.11",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "moment-mini": "^2.22.1",
    "ngx-quill": "^14.3.0",
    "quill": "^1.3.6",
    "quill-delta": "^3.6.2",
    "quill-image-drop-module": "git+https://github.com/Smip/quill-image-drop-module.git",
    "quill-image-upload": "^0.1.3",
    "reflect-metadata": "0.1.13",
    "request": "^2.88.0",
    "rxjs": "6.6.7",
    "rxjs-tslint": "0.1.7",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.18",
    "@angular/cli": "11.2.18",
    "@angular/compiler-cli": "11.2.14",
    "@angular/language-service": "11.2.14",
    "@angular/platform-server": "11.2.14",
    "@nguniversal/builders": "^11.2.1",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "11.13.7",
    "@types/quill": "^1.3.6",
    "chai": "4.2.0",
    "codelyzer": "^6.0.0",
    "cross-env": "5.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.11",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "6.1.4",
    "nodemon": "1.18.11",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "2.2.1",
    "protractor": "~7.0.0",
    "source-map-support": "0.5.19",
    "ssri": "8.0.1",
    "ts-loader": "^5.2.2",
    "ts-mocha": "6.0.0",
    "ts-node": "8.1.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.8",
    "webpack": "^4.46.0",
    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2"
  }
}
