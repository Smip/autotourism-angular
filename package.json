{
  "name": "autotourism",
  "version": "8.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ssr": "npm run build:universal && npm run generate:prerender && npm run server",
    "ssr:debug": "ng build --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true  && ng run autotourism:server:dev && webpack && node server.js",
    "ssr:cw": "ng build --aot=true --output-hashing=all --named-chunks=false --build-optimizer=true --watch",
    "ssr:sw": "ng run autotourism:server:dev --watch",
    "ssr:webpack": "webpack --watch",
    "ssr:server": "nodemon server.js",
    "ssr:watch": "run-p ssr:universal:build:*",
    "ssr:universal:build:browser": "ng run autotourism:build:dev --watch",
    "ssr:universal:build:server": "node ./node_modules/npm-delay 15000 && ng run autotourism:server:dev --watch",
    "ssr:universal:build:webpack": "node ./node_modules/npm-delay 30000 && webpack --config webpack.config.js --watch",
    "ssr:universal:build:nodemon": "node ./node_modules/npm-delay 35000 && nodemon --inspect server.js",
    "format:check": "prettier --write --config ./.prettierrc --list-different \"src/{app,environments,styles}/**/*{.ts,.json,.scss}\"",
    "build": "ng build",
    "build:server": "ng run autotourism:server:production",
    "build:prod": "ng build --prod",
    "build:universal": "ng run autotourism:build:production && ng run autotourism:server:production && webpack --progress --colors",
    "server": "node server.js",
    "test": "ng test",
    "test:ssr": "run-p test:ssr:*",
    "test:ssr:server": "cross-env TEST=true node server.js",
    "test:ssr:script": "node ./node_modules/npm-delay 2000 && ts-mocha test/*.spec.ts test/exit.ts --timeout 15000",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prerender": "npm run build:universal && npm run generate:prerender",
    "generate:prerender": "node prerender.js",
    "prerender": "ng run autotourism:prerender",
    "postinstall": "ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.5",
    "@angular/common": "10.2.5",
    "@angular/compiler": "10.2.5",
    "@angular/core": "10.2.5",
    "@angular/forms": "10.2.5",
    "@angular/platform-browser": "10.2.5",
    "@angular/platform-browser-dynamic": "10.2.5",
    "@angular/pwa": "0.13.8",
    "@angular/router": "10.2.5",
    "@angular/service-worker": "10.2.5",
    "@gorniv/ngx-universal": "^2.2.2",
    "@nguniversal/common": "9.1.1",
    "@nguniversal/express-engine": "^9.1.1",
    "@ngx-meta/core": "9.0.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@smip/ngx-materialize": "^0.2.5",
    "angulartics2": "^10.1.0",
    "cookie-parser": "1.4.4",
    "core-js": "^2.6.11",
    "materialize-css": "^1.0.0",
    "moment": "^2.24.0",
    "moment-mini": "^2.22.1",
    "ngx-quill": "^12.0.1",
    "quill": "^1.3.6",
    "quill-delta": "^3.6.2",
    "quill-image-drop-module": "git+https://github.com/Smip/quill-image-drop-module.git",
    "quill-image-upload": "^0.1.3",
    "reflect-metadata": "0.1.13",
    "request": "^2.88.0",
    "rxjs": "6.6.7",
    "rxjs-tslint": "0.1.7",
    "tslib": "^2.0.0",
    "zone.js": "0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1002.4",
    "@angular/cli": "10.2.4",
    "@angular/compiler-cli": "10.2.5",
    "@angular/language-service": "10.2.5",
    "@angular/platform-server": "10.2.5",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/jasmine": "3.3.12",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "11.13.7",
    "@types/quill": "^1.3.6",
    "chai": "4.2.0",
    "codelyzer": "5.0.1",
    "cross-env": "5.2.0",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "mocha": "6.1.4",
    "nodemon": "1.18.11",
    "npm-delay": "1.0.4",
    "npm-run-all": "4.1.5",
    "prettier": "1.17.0",
    "protractor": "~7.0.0",
    "source-map-support": "0.5.12",
    "ssri": "6.0.1",
    "ts-loader": "^5.2.2",
    "ts-mocha": "6.0.0",
    "ts-node": "8.1.0",
    "tslint": "~6.1.0",
    "typescript": "4.0.8",
    "webpack": "^4.46.0",
    "webpack-cli": "3.3.1",
    "webpack-node-externals": "1.7.2"
  }
}
